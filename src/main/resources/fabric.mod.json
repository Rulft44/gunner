{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${version}",
	"name": "${modName}",
	"description": "${modDescription}",
	"authors": [
		"${authors}"
	],
	"contact": {
		"homepage": "${homepage}",
		"issues": "${issues}",
		"sources": "${sources}"
	},
	"license": "${license}",
	"icon": "assets/${modId}/icon.png",
	"mixins": [
		"${modId}.mixins.json"
	],
	"depends": {
		"minecraft": ">=${mc}",
		"fabricloader": ">=${fl}",
		"fabric-api": ">=${fapi}",
		"cloth-config2": ">=${cloth}",
		"geckolib": ">=${geckolib}",
		"confetti": ">=${confetti}"
	},
	"suggests": {
		"enchdesc": "*",
		"modmenu": ">=${modmenu}"
	},
	"entrypoints": {
		"main": [
			"io.github.rulft44.gunner.Gunner"
		],
		"client": [
		],
		"fabric-datagen": [
			"io.github.rulft44.gunner.ModDataGenerator"
		],
		"modmenu": [
			"io.github.rulft44.gunner.compat.GunnerModMenuIntegration"
		]
	}
}
